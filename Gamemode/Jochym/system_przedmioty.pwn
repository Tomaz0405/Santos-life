forward PrzedmiotyRosliny(pokazany, playerid, dialogid, naglowek[], typ, dzialalnosc);
public PrzedmiotyRosliny(pokazany, playerid, dialogid, naglowek[], typ, dzialalnosc)
{
	new item_list[256], items_list[2048], find;
	if(typ == TYP_MAGAZYN)
	{
	    format(items_list, sizeof(items_list), "UID\t\tIloœæ\t\tCena\t\tNazwa", items_list);
	}
	else
	{
	    format(items_list, sizeof(items_list), "UID\tNazwa przedmiotu\tWartosci\tCena", items_list);
		ForeachEx(i, MAX_PRZEDMIOT)
		{
			if(typ == TYP_WLASCICIEL)
			{
			    if(PrzedmiotInfo[i][pOwner] == DaneGracza[playerid][gUID] && PrzedmiotInfo[i][pUID] != 0 && PrzedmiotInfo[i][pTypWlas] == TYP_WLASCICIEL && PrzedmiotInfo[i][pTyp] == P_ROSLINA || PrzedmiotInfo[i][pOwner] == DaneGracza[playerid][gUID] && PrzedmiotInfo[i][pUID] != 0 && PrzedmiotInfo[i][pTypWlas] == TYP_WLASCICIEL && PrzedmiotInfo[i][pTyp] == P_SKLADNIK_MARYCHA)
				{
					new cena;
					if(PrzedmiotInfo[i][pTyp] == P_ROSLINA)
					{
						cena = 200;
					}
					else if(PrzedmiotInfo[i][pTyp] == P_SKLADNIK_MARYCHA)
					{
						cena = 300;
					}
					format(item_list, sizeof(item_list), "%d\t{FFFFFF}%s\t{222222}[%d, %d, %d]\t{7ce593}${FFFFFF}%d", PrzedmiotInfo[i][pUID], PrzedmiotInfo[i][pNazwa], PrzedmiotInfo[i][pTyp], PrzedmiotInfo[i][pWar1], PrzedmiotInfo[i][pWar2], cena);
					format(items_list, sizeof(items_list), "%s\n%s", items_list, item_list);
					find++;
				}
			}
		}
	}
	if(find == 0)
	{
		GameTextForPlayer(pokazany, "~w~~h~Nie ~r~znaleziono ~w~zadnych roslin!", 3000, 5);
	}
	else
	{
 		dShowPlayerDialog(pokazany, dialogid, DIALOG_STYLE_TABLIST_HEADERS, naglowek, items_list, "Sprzedaj", "Zamknij");
	}
	return 1;
}
