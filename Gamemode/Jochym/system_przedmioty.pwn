forward UsunPrzedmiot(uid);
public UsunPrzedmiot(uid)
{
	format(zapyt, sizeof(zapyt), "DELETE FROM `jochym_przedmioty` WHERE `ID` = '%d'", uid);
	mysql_check();
	mysql_query2(zapyt);
	DestroyDynamicObject(PrzedmiotInfo[uid][pIDOB]);
    PrzedmiotInfo[uid][pUID] = 0;
    PrzedmiotInfo[uid][pOwner] = -1;
    PrzedmiotInfo[uid][pTypWlas] = -1;
    PrzedmiotInfo[uid][pTyp] = 0;
    PrzedmiotInfo[uid][pWar1] = 0;
	PrzedmiotInfo[uid][pWar2] = 0;
	PrzedmiotInfo[uid][pWar4] = 0;
	PrzedmiotInfo[uid][pWar5] = 0;
	format(PrzedmiotInfo[uid][pWar3], 32, "%s", 0);
	PrzedmiotInfo[uid][pX] = 0;
	PrzedmiotInfo[uid][pY] = 0;
	PrzedmiotInfo[uid][pZ] = 0;
    PrzedmiotInfo[uid][pVW] = 0;
	PrzedmiotInfo[uid][pTermin] = 0;
	PrzedmiotInfo[uid][pStworzylUID] = -1;
	PrzedmiotInfo[uid][pWaga] = 0;
	PrzedmiotInfo[uid][pObject] = 0;
	PrzedmiotInfo[uid][pNazwa] = 0;
	PrzedmiotInfo[uid][pUzywany] = 0;
	PrzedmiotInfo[uid][pCenas] = 0;
	mysql_free_result();
}
